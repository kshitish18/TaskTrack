{
    "version": 2,
    "builds": [
      { "src": "build_files.sh", "use": "@vercel/static-build", "config": { "distDir": "staticfiles" } },
      { "src": "todo_main/wsgi.py", "use": "@vercel/python", "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" } }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "todo_main/wsgi.py" }
    ]
  }
  